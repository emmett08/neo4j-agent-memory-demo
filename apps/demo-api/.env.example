NEO4J_URI=neo4j://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# OpenAI (default provider)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Optional: Use Auggie instead of OpenAI
# Set AUGGIE_ENABLE=1 to force Auggie without an OpenAI key.
# Auggie SDK Authentication (supports 3 methods):
#
# Method 1 (Recommended): Direct API Key
# Get your token by running: auggie token print
# Use the "accessToken" value from the output
# AUGMENT_API_TOKEN=your_access_token_here
# Optional: Specify API URL (use "tenantURL" from auggie token print)
# AUGMENT_API_URL=https://e5.api.augmentcode.com/
#
# Method 2: Environment Variables Only
# Set AUGMENT_API_TOKEN and AUGMENT_API_URL as shown above
#
# Method 3: settings.json (automatic)
# If no env vars are set, SDK will automatically load from:
# - macOS/Linux: ~/.config/auggie/settings.json
# - Windows: %APPDATA%\auggie\settings.json
# Run 'auggie token print' to see your current settings

# Auggie Model Configuration
# AUGGIE_MODEL=sonnet4.5
# AUGGIE_ENABLE=0

# Server Configuration
PORT=8080
